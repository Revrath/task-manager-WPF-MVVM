<Window x:Class="TaskManager.View.MainWin"
        xmlns:controls="clr-namespace:TaskManager.uc"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TaskManager.View"
        xmlns:viewModel="clr-namespace:TaskManager.ViewModel"
        xmlns:diagnostics="clr-namespace:System.Diagnostics;assembly=System"
        xmlns:taskManager="clr-namespace:TaskManager"
        mc:Ignorable="d"
        Title="MainWin" Height="800" Width="650"
        >

    <Grid>
        <TabControl>
            <TabItem Header="Processes">
                <local:ProcessesTab></local:ProcessesTab>
            </TabItem>
            <TabItem Header="Tasks">
                <local:TasksTab></local:TasksTab>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
